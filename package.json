{
  "name": "purescript-halogen-mdl",
  "version": "0.1.0",
  "description": "Material Design Lite components for Halogen",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "build": "npx -c 'spago build'",
    "build:examples": "pulp build --src-path examples/src --include src --to examples/dist/app.js",
    "watch:examples": "pulp --watch build --src-path examples/src --include src --to examples/dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andywhite37/purescript-halogen-mdl.git"
  },
  "keywords": [
    "purescript",
    "halogen",
    "mdl"
  ],
  "author": "Andy White",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andywhite37/purescript-halogen-mdl/issues"
  },
  "homepage": "https://github.com/andywhite37/purescript-halogen-mdl#readme",
  "devDependencies": {
    "purescript": "^0.12.5",
    "purescript-psa": "^0.5.1",
    "spago": "^0.8.3"
  }
}
